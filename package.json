{
  "name": "fm-gofer",
  "version": "0.3.2",
  "description": "Framework for calling FileMaker scripts from a webviewer and retrieving results using callback promises.",
  "main": "dist/fm-gofer.js",
  "module": "dist/fm-gofer.mjs",
  "scripts": {
    "test": "mocha --require esm",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha",
    "build": "webpack",
    "jsdoc": "jsdoc -r src -t node_modules/tsd-jsdoc/dist -d dist --package package.json"
  },
  "author": "Josh Willing <josh@willingapps.com>",
  "license": "ISC",
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/register": "^7.13.14",
    "babel-loader": "^8.2.2",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.22.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "esm": "^3.2.25",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "tsd-jsdoc": "^2.5.0",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.5.0"
  }
}
