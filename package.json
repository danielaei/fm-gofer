{
  "name": "fm-gofer",
  "version": "1.6.2",
  "description": "Framework for calling FileMaker scripts from a webviewer and retrieving results using callback promises.",
  "homepage": "https://github.com/jwillinghalpern/fm-gofer#readme",
  "issues": "https://github.com/jwillinghalpern/fm-gofer/issues",
  "source": "src/index.ts",
  "main": "dist/fm-gofer.js",
  "module": "dist/fm-gofer.mjs",
  "types": "dist/fm-gofer.d.ts",
  "browserslist": "defaults",
  "exports": {
    ".": {
      "import": "./dist/fm-gofer.mjs",
      "require": "./dist/fm-gofer.js",
      "default": "./dist/fm-gofer.js"
    },
    "./polyfill-ie11": "./dist/polyfill-ie11.js"
  },
  "files": [
    "dist/*",
    "__tests__/*",
    "src/*"
  ],
  "keywords": [
    "filemaker",
    "javascript",
    "typescript",
    "web viewer",
    "webviewer",
    "promise",
    "callback"
  ],
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --collect-coverage",
    "build": "rollup -c && node scripts/rename-dts.js && npm run build-example",
    "build-example": "npx gulp"
  },
  "author": "Josh Willing Halpern <josh@willingapps.com>",
  "license": "ISC",
  "sourceType": "module",
  "sideEffects": false,
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@types/jest": "^29.0.0",
    "@types/jsdom": "^20.0.0",
    "babel-plugin-rewire-ts": "^1.4.0",
    "core-js": "^3.25.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gulp": "^4.0.2",
    "gulp-inline-source": "^4.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "http-server": "^14.1.1",
    "jest": "^29.0.2",
    "jest-environment-jsdom": "^29.0.2",
    "jest-environment-jsdom-global": "^4.0.0",
    "prettier": "^2.7.1",
    "rollup": "^2.79.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.33.0",
    "typescript": "^4.8.2",
  }
}
